int Led1=1;//
int Led2=2;//
int Led3=3;//


void setup()
 
{
  pinMode(Led1, OUTPUT);
  pinMode(Led3 , OUTPUT);
  pinMode (Led2 ,OUTPUT);
}

void loop()
{
  digitalWrite(Led1, HIGH);
  delay(5000);
  digitalWrite(Led1, LOW);
  delay(5000); 
  
  
   digitalWrite(Led2, HIGH);
  delay(2000);
  digitalWrite(Led2, LOW);
  delay(2000);
  
  
  
  digitalWrite(Led3 , HIGH);
  delay(5000);
  digitalWrite(Led3, LOW);
  delay(5000);
}
